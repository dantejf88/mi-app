{
  "name": "mi-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "env-cmd": "^8.0.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.3"
  },
  "scripts": {
    "start": "env-cmd ./env-variables/.env.development react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build:staging": "env-cmd ./env-variables/.env.staging npm run build",
    "build:production": "env-cmd ./env-variables/.env.production npm run build",
    "build:development": "env-cmd ./env-variables/.env.development npm run build"
  },
  "proxy": "http://localhost:8080",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
