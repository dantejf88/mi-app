{
  "name": "mi-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "env-cmd": "^8.0.2",
    "eslint-config-standard-react": "^7.0.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts": "2.0.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "env-cmd ./env-variables/.env.development react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build:staging": "env-cmd ./env-variables/.env.staging npm run build",
    "build:production": "env-cmd ./env-variables/.env.production npm run build",
    "build:development": "env-cmd ./env-variables/.env.development npm run build"
  },
  "proxy": "http://localhost:3001",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint-plugin-react": "^7.11.1",
    "react-loadable": "^5.5.0"
  }
}
